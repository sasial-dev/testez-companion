        <string name="Name">extractUsefulKeys</string>
        <string name="Source"><![CDATA[-- WARNING: recursing more than 5000 times will cause a stack overflow. Help wanted
local function extractUsefulKeys(value, key)
	if
		typeof(key) == "string"
		and not (
			key == "children"
			or key == "errors"
			or key == "failureCount"
			or key == "skippedCount"
			or key == "successCount"
			or key == "planNode"
			or key == "phrase"
			or key == "type"
			or key == "status"
			or key == "modifier"
		)
	then
		return nil
	end

	if typeof(value) == "table" then
		for k, v in pairs(value) do
			value[k] = extractUsefulKeys(v, k)
		end
	end

	return value
end

return extractUsefulKeys
]]></string>
